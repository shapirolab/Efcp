-language([nil]).
-mode(interrupt).

LF => 10.	% ascii value of line-feed <lf>
BLANK => 32.	% ascii value of blank
AT_SIGN => 64.	% ascii value for @
DOT => 46.	% ascii value for .
PERCENT => 37.  % ascii value for %
HASH => 35. 	% ascii for #

SIGNIFICANT_DIGITS => 7.
